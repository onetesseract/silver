main (argc int, argv &&char) int = {
  while argc != 0 {
    print(argv[argc])
    argc--
  }
}

// if functions have two arguments they can be defined on the class as the operation (like +)
// alternatively i could call it as c1.+(c2)

complex class = {
  x float
  y float
  self complex + complex(adder complex) = { complex{x = self.x + adder.x, y = self.y + adder.y} }
}

(self complex) + (adder complex) complex = complex{x = self.x + adder.x, y = self.y + adder.y}
